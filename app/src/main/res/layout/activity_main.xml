<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/principal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/direccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingresa tu dirección"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="88dp" />

    <TextView
        android:id="@+id/telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingresa tu telefono"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="153dp" />

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ingresa tu email"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="202dp" />

    <TextView
        android:id="@+id/documento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ingresa tu documento"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="256dp" />

    <TextView
        android:id="@+id/edad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ingresa tu edad"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="315dp" />

    <Button
        android:id="@+id/btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Regístrame"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="411dp" />
</androidx.constraintlayout.widget.ConstraintLayout>